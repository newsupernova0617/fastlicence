# SvelteKit public env vars (Vite-style, always sent to client)
PUBLIC_SUPABASE_URL=https://your-project.supabase.co
PUBLIC_SUPABASE_ANON_KEY=replace-with-anon-key

# OAuth provider IDs (frontend-only)
PUBLIC_GOOGLE_CLIENT_ID=
PUBLIC_KAKAO_CLIENT_ID=

# Optional: override backend auth API host (defaults to current origin)
PUBLIC_AUTH_API_URL=

# Optional: adjust mock redirect target when running dev server
